generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Drug {
  id               Int    @id @default(autoincrement())
  remoteKey        Int    @unique
  brand            String
  imageUrl         String
  price            Int
  offerPrice       Int
  shortDescription String
  longDescription  String
}
